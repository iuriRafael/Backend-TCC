{
    "version": 2,
    "builds": [
        {
            "src":"/index.js",
            "use":"@vercel/node"
        }
    ],
    "routes": [
        {
            "src":"(.*)",
            "dest":"/index.js"
        }
    ],

    "rewrites": [
        {
          "source": "/uploads/(.*)",
          "destination": "https://backend-tcc-rho.vercel.app/posts/upload"
        }
      ]

    
    
}